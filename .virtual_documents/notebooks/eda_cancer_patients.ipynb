warnings.filterwarnings('ignore')

# Set style for better-looking plots
sns.set_style("whitegrid")
plt.rcParams['figure.figsize'] = (12, 8)

print("Libraries imported successfully!")


import os

print(os.getcwd())



# Load the data
df = pd.read_csv('../data/raw/lung_cancer.csv')

print("=" * 60)
